      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.shengfeng.firstreactnative"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "62ed477d-fcc7-4113-a1e6-8e1d73167a76"
      }
    },
    "owner": "shengfeng"
  }
}
